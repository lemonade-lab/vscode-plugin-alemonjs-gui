{
  "name": "alemonjs-gui",
  "displayName": "ALemonJS GUI",
  "publisher": "lemonade-x",
  "description": "阿柠檬机器人可视化界面",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "resources/favicon.ico",
  "main": "./dist/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "alemonjs.openChatSidebar",
        "title": "打开聊天侧边栏"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "alemonjs",
          "title": "AlemonJS",
          "icon": "$(comment-discussion)"
        }
      ]
    },
    "views": {
      "alemonjs": [
        {
          "type": "webview",
          "id": "alemonjs.chatSidebar",
          "name": ""
        }
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "eslint": "eslint . --fix",
    "preview": "vite preview",
    "format": "prettier --write .",
    "check-format": "git diff --exit-code",
    "vscode:prepublish": "yarn run package",
    "compile": "yarn run check-types && yarn run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "yarn run check-types && yarn run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.13",
    "@babel/eslint-plugin": "^7.12.13",
    "@ecomfe/eslint-config": "^7.0.0",
    "@ecomfe/stylelint-config": "^1.0.0",
    "@eslint/js": "^9.14.0",
    "@reduxjs/toolkit": "^2.4.0",
    "@tailwindcss/forms": "^0.5.9",
    "@types/lodash-es": "^4.17.12",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/qs": "^6.9.17",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@uiw/react-markdown-preview": "^5.1.3",
    "@vitejs/plugin-react": "^4.2.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.19",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "echarts-for-react": "^3.0.2",
    "esbuild": "^0.24.0",
    "eslint": "^9.16.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-stylelint": "^0.1.1",
    "globals": "^15.12.0",
    "immer": "^10.1.1",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "preline": "^2.1.0",
    "prettier": "^3.3.3",
    "qs": "^6.13.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.28.0",
    "rehype-attr": "^3.0.3",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-highlight": "^7.0.1",
    "rehype-prism": "^2.3.3",
    "rehype-raw": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "sass": "^1.75.0",
    "stylelint": "^14.9.1",
    "tailwindcss": "^3.4.3",
    "terser": "^5.30.4",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.14.0",
    "use-immer": "^0.9.0",
    "vite": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemonade-lab/vscode-plugin-alemonjs-gui.git"
  }
}
